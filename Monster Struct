int main()
{
    monsters mewtwo = {8,6,7,"Mewtwo"};
    printf("Name= %s, Health= %d, Mind Control= %d, Skill= %d",mewtwo.mname, mewtwo.mhealth,mewtwo.mattack,mewtwo.mskill);
    return 0;
