#include <stdio.h>

typedef struct monsters monsters;
struct monsters
{
    int mHP;
    int mATK;
    int mskill;
    char mname[50];
};

int main()
{
monsters rock = {10,1,5,"A small rock"};
monsters flytrap={5,3,5,"A Venus Flytrap"};
monsters bluescreen={100,0,25,"The Blue Screen of Death"};
monsters killerrabbit={15,20,100,"A small rabbit, with a killer streak a mile wide"};
monsters blackknight={100,10,60,"The Black Knight-'Tis But a Scratch!"};
monsters zombie={25,10,30,"The Walking Dead"};
monsters satan={1000000,200,1000,"Literally Satan. The devil. Hell incarnate."};
monsters whitegirls={50,2,20,"A wild pack of drunk white girls"};
monsters moon={1000,20,100,"The moon. The hunk of rock past the atmosphere."};
monsters mastering={200,18,50,"MasteringPhysics.com."};

monsters DM = {30,40,20, "Dark Magician"}
monsters BEWD = {20,20,30, "Blue Eyes White Dragon"}
monsters REBD = {30,20,20, "Red Eyes Bkack Dragon"}
monsters GSS = {400,10,20, "Giant Soldier of Stone"}
monsters NMS = {20,25,15, "Neo the Magic Swordsman"}
monsters EFO = {50,50,50, "Exodia the Forbidden One"}
monsters DMG = {30,30,20, "Dark Magician Girl"}
monsters SG = {10,10,20, "Scapegoat"}
monsters SD = {30,30,30, "Stardust Dragon"}
monsters CD = {20,20,20, "Cyber Dragon"}
return 0;
}


#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mhealth;
    int mattack;
    int mskill;
    char mname[20];
};

int main()
{
    monsters jynx = {8,6,7,"Jynx"};
    printf("Name= %s, Health= %d, Humiliate= %d, Skill= %d",jynx.mname, jynx.mhealth,jynx.mattack,jynx.mskill);
    return 0;
}




#include <stdio.h>
typedef struct monsters monsters;
struct monsters
{
    int mhealth;
    int mattack;
    int mskill;
    char mname[20];
};

int main()
{
    monsters pikachu = {5,2,3,"Pikachu"};
    printf("Name= %s, Health= %d, Thunderbolt= %d, Skill= %d",pikachu.mname, pikachu.mhealth,pikachu.mattack,pikachu.mskill);
    return 0;
}




